<Window x:Class="Flappy_bird.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Flappy_bird"
        mc:Ignorable="d"
        Title="MainWindow" Height="900" Width="1600" Foreground="{x:Null}" Background="#FF1A1A1A" KeyDown="Window_KeyDown" KeyUp="Window_KeyUp">
    <Grid>
        <Canvas x:Name="field" Height="800" VerticalAlignment="Top">
            <Canvas.Background>
                <ImageBrush ImageSource="/625217.jpg" Stretch="UniformToFill"/>
            </Canvas.Background>
        </Canvas>

    </Grid>
</Window>
